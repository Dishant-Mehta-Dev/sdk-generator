{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../dist/tools-generator",
    "rootDir": "..",
    "baseUrl": ".",
    "paths": {
      "*": ["*", "../*"]
    }
  },
  "include": ["./**/*", "../generated/**/*"],
  "exclude": ["node_modules"]
}
